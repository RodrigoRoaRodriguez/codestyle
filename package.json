    "Mikael Abrahamsson <mikael.abrahamsson@gutro.se>",
    "Sebastian Stenerdal <sebastian.stenerdal@gutro.se>",
    "Enzo Martin <enzo.martin@gutro.se>"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^6.0.0",
    "eslint": "^3.0.1",
    "estraverse-fb": "^1.3.1"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "istanbul": "^0.4.2"
  }
}
