{
  "name": "codestyle",
  "version": "8.0.0",
  "repository": "git@github.com:LeoGears/codestyle.git",
  "description": "ESLint configuration used at Gears of Leo",
  "scripts": {
    "lint": "eslint test/* *.js",
    "test": "istanbul cover node_modules/mocha/bin/_mocha -- --full-trace",
    "ci": "istanbul cover node_modules/mocha/bin/_mocha -- -R tap > test-results.tap && istanbul report clover",
    "prepublish": "npm run lint && npm run test"
  },
