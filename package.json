{
  "name": "codestyle",
  "version": "21.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeoGears/codestyle.git"
  },
  "description": "ESLint configuration used at Gears of Leo",
  "scripts": {
    "lint": "node_modules/.bin/eslint test/* *.js",
    "lint-react": "node_modules/.bin/eslint -c .react-eslintrc test/* *.js",
    "lint-fix": "node_modules/.bin/eslint test/* *.js --fix",
    "test": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- --full-trace test/* test/**/*",
    "ci": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -R tap test/* test/**/* > test-results.tap && node_modules/.bin/istanbul report clover",
    "prepublish": "npm run lint && npm run lint-react && npm run test"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "contributors": [
    "Mikael Abrahamsson <mikael.abrahamsson@leovegas.com>",
    "Sebastian Stenerdal <sebastian.stenerdal@leovegas.com>",
    "Enzo Martin <enzo.martin@leovegas.com>"
  ],
  "keywords": [
    "codestyle",
    "eslint",
    "eslint rules",
    "rules",
    "es6",
    "react"
  ],
  "license": "ISC",
  "dependencies": {
    "babel-eslint": "^7.1.1"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2"
  },
  "peerDependency": {
    "eslint": ">= 4"
  }
}
